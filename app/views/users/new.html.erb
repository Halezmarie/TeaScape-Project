<h1>Do you want to become a TeaScape member? Sign up here!:</h1>
<%= form_with model: @user do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Sign Up' %>
<% end %>

<h2>Login with Google:</h2>

<%= button_to 'Click to login with Google', '/auth/google_oauth2' %>
